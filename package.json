    "csv-parse": "^1.1.7",
    "electron": "^1.3.4",
    "events": "^1.1.1",
    "mongo-sync": "^2.0.1",
    "mongod": "^1.3.0",
    "mongodb": "^2.2.9",
    "mongoose": "^4.5.10",
    "primeng": "^1.0.0-beta.13",
    "primeui": "^4.1.15",
